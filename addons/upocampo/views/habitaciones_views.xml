<odoo>
  <record id="habitaciones_form_view" model="ir.ui.view">
    <field name="name">upocampo.habitaciones.form</field>
    <field name="model">upocampo.habitaciones</field>
    <field name="arch" type="xml">
      <form string="Classes">
        <sheet layout="auto">
          <group>
            <field name="name"/>
            <field name="foto"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

  <record id="habitaciones_tree_view" model="ir.ui.view">
    <field name="name">upocampo.habitaciones.tree</field>
    <field name="model">upocampo.habitaciones</field>
    <field name="arch" type="xml">
      <tree string="tree_string">
        <field name="id"/>
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record id="habitaciones_list_action" model="ir.actions.act_window">
    <field name="name">Habitaciones</field>
    <field name="res_model">upocampo.habitaciones</field>
    <field name="view_mode">tree,form</field>
    <field name="help" type="html">
      <p class="o_view_nocontent_smiling_face">Create the firstclass</p>
    </field>
  </record>
</odoo>